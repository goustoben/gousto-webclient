{
  "name": "gousto-regression",
  "version": "1.0.0",
  "description": "Gousto regression",
  "main": "index.js",
  "scripts": {
    "test:debug:web": "NODE_OPTIONS=--max-http-header-size=1000000 cypress open --env platform=web",
    "test:debug:mobile": "NODE_OPTIONS=--max-http-header-size=1000000 CYPRESS_viewportWidth=375 CYPRESS_viewportHeight=667 cypress open --env platform=mobile",
    "test:web": "NODE_OPTIONS=--max-http-header-size=1000000 cypress run --env platform=web",
    "test:web:single": "NODE_OPTIONS=--max-http-header-size=1000000 cypress run --spec",
    "test:mobile": "NODE_OPTIONS=--max-http-header-size=1000000 CYPRESS_viewportWidth=375 CYPRESS_viewportHeight=667 cypress run --env platform=mobile",
    "test:staging:web": "NODE_OPTIONS=--max-http-header-size=1000000 cypress run --env platform=web",
    "test:staging:mobile": "NODE_OPTIONS=--max-http-header-size=1000000 CYPRESS_viewportWidth=375 CYPRESS_viewportHeight=667 cypress run --env platform=mobile"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^7.2.0",
    "moment": "^2.25.3"
  },
  "devDependencies": {
    "cypress-pipe": "^1.7.0"
  }
}
