{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": ["error", "never"],
    "import/no-unresolved": "off",
    "react/jsx-indent-props": ["error", "tab"],

    "max-len": "off",
    "no-shadow": ["error", { "allow": ["resolve", "reject", "done", "cb", "e", "err", "error", "mapStateToProps"] }],
    "no-underscore-dangle": ["error", { "allow": ["__initialState__"] }],
    "new-cap": 0,
    "no-tabs": 0,
    "newline-before-return": "error",
    "no-multiple-empty-lines": 0,
    "arrow-parens": 0,
    "new-parens":0,
    "implicit-arrow-linebreak": 0,
    "no-else-return": 0,
    "prefer-destructuring": 0,
    "no-useless-escape": 0,
    "no-plusplus": 0,
    "operator-linebreak": 0,
    "object-curly-newline": 0,
    "react/require-default-props": 0,
    "react/no-deprecated": 0,
    "import/order": 0,
    "react/jsx-wrap-multilines": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "react/jsx-closing-tag-location": 0,
    "react/destructuring-assignment": 0,
    "react/no-unused-prop-types": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "react/forbid-prop-types": 0,
    "comma-dangle": 0,
    "function-paren-newline": 0,
    "react/jsx-tag-spacing": 0,
    "react/self-closing-comp": 0,
    "react/self-closing-import/no-dynamic-require": 0,
    "lines-between-class-members": 0,
    "jsx-a11y/label-has-for": 0,
    "react/sort-comp": 0,
    "react/jsx-curly-brace-presence": 0,
    "react/default-props-match-prop-types": 0,
    "comma-style": 0,
    "react/style-prop-object": 0,
    "padded-blocks": 0,
    "react/no-unescaped-entities": 0,
    "react/no-array-index-key": 0,
    "no-multi-spaces": 0,
    "react/prop-types": 0,
    "react/no-typos": 0,
    "react/jsx-indent": 0,
    "react/no-string-refs":0,
    "react/jsx-no-target-blank":0,
    "no-prototype-builtins": 0,
    "no-extra-boolean-cast": 0,
    "import/no-useless-path-segments": 0,
    "import/extensions": 0,
    "prefer-const": 0,
    "class-methods-use-this": 0,
    "space-unary-ops": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-props-no-multi-spaces": 0,
    "react/no-redundant-should-component-update": 0,
    "import/named": 0,
    "import/no-self-import": 0,
    "import/no-named-as-default-member": 0,
    "import/newline-after-import": 0,
    "no-lonely-if": 0,
    "import/first": 0,
    "react/no-find-dom-node": 0,
    "react/no-unused-state": 0,
    "react/prefer-stateless-function":0 ,
    "import/no-extraneous-dependencies": 0,
    "no-return-await": 0,
    "react/jsx-no-duplicate-props": 0,
    "no-undef": 0,
    "prefer-promise-reject-errors": 0,
    "no-restricted-syntax": 0,
    "react/button-has-type": 0,
    "import/no-cycle": 0,
    "no-restricted-globals": 0,
    "no-mixed-operators": 0,
    "react/jsx-curly-spacing": 0,
    "jsx-a11y/interactive-supports-focus": 0,
    "react/no-access-state-in-setstate": 0,
    "jsx-a11y/no-autofocus": 0,
    "jsx-a11y/anchor-has-content": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "no-bitwise": 0,
    "no-control-regex": 0,
    "react/no-children-prop": 0
    
  },
  "env" : {
    "jasmine": true
  },
  "globals": {
    "document": true,
    "navigator": true,
    "require": false,
    "window": true,
    "__DEV__": false,
    "__PROD__": false,
    "__HMR__": false,
    "__SERVER__": false,
    "__CLIENT__": false,
    "__ENV__": false,
    "__DOMAIN__": false,
    "__CLIENT_PROTOCOL__": false,
    "__CLOUDFRONT_URL__": false,
    "__TEST__": false,
    "__GIT_HASH__": false
  }
}
