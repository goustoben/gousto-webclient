    "react-router-scroll": "0.4.1",
    "react-select": "1.0.0-beta13",
    "react-slick": "0.14.5",
    "recompose": "0.27.1",
    "redux": "3.5.2",
    "redux-form": "6.7.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "3.0.1",
    "scroll": "2.0.3",
    "scroll-into-view": "1.8.0",
    "seedrandom": "2.4.3",
    "transit-js": "0.8.861",
    "transit-immutable-js": "0.7.0",
    "url": "0.11.0",
