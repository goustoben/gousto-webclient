{
  "name": "goustowebclient",
  "version": "0.0.1",
  "description": "Isomorphic Gousto WebClient",
  "main": "index.js",
  "author": "Gousto",
  "license": "ISC",
  "sideEffects": "false",
  "scripts": {
    "start": "node dist/server.js",
    "start:with-static": "withStatic=true yarn run start",
    "start:dev": "nodemon dist/server.js -L",
    "start:devlocal": "nodemon dist/server.js",
    "start:hmr": "NODE_ENV=hmr yarn run build:server && yarn run start",
    "watch:client": "webpack --watch --config config/webpack.client.js",
    "watch:server": "webpack --watch --config config/webpack.server.js",
    "watch": "rm -rf dist/* && rm -rf public/* && run-p watch:client watch:server",
    "build:dev": "rm -rf dist/* && rm -rf public/* && run-p build:server build:client",
    "build:client": "webpack --config config/webpack.client.js --config-name=client --bail",
    "build:server": "webpack --config config/webpack.server.js --config-name=server --bail",
    "build:client:prod": "rm -rf public/* && NODE_ENV=production webpack --config config/webpack.client.js --bail",
    "build:server:prod": "rm -rf dist/* && NODE_ENV=production webpack --config config/webpack.server.js --bail",
    "build:e2e": "NODE_ENV=staging webpack --config config/webpack.e2e.js --bail",
    "build:e2e:local": "NODE_ENV=local webpack --config config/webpack.e2e.js --bail",
    "build": "rm -rf dist/* && rm -rf public/* && NODE_ENV=production run-p build:server build:client",
    "build:docker:dev": "rm -rf dist/* && rm -rf public/* && NODE_ENV=production npm_config_gousto_webclient_environment_name=local npm_config_gousto_webclient_domain=gousto.local run-p build:server build:client",
    "test:jest": "jest",
    "test:jest:ci": "jest --ci -w=4",
    "test:jest:one": "jest",
    "test:jest:one:watch": "jest --watch",
    "test:jest:watch": "jest --watchAll",
    "test:jest:coverage": "jest --coverage",
    "test:jest:ci:coverage": "jest --ci -w=4 --coverage",
    "test:mocha": "mocha-webpack --webpack-config config/webpack.test-mocha.js -r \"test/.setup.js\" \"test/**/*.js\"",
    "test:mocha:one": "mocha-webpack --webpack-config config/webpack.test-mocha.js --recursive -r \"test/.setup.js\"",
    "test:mocha:one:watch": "mocha-webpack --webpack-config config/webpack.test-mocha.js --watch -r \"test/.setup.js\"",
    "test:mocha:watch": "mocha-webpack --webpack-config config/webpack.test-mocha.js --watch -r \"test/.setup.js\" \"test/**/*.js\"",
    "test:mocha:coverage": "NODE_ENV=test nyc mocha-webpack --webpack-config config/webpack.test-mocha.js -r \"test/.setup.js\" \"test/**/*.js\" && nyc report --reporter=html",
    "deploy:sentry-upload": "chmod a+x getsentryUpload.sh && ./getsentryUpload.sh",
    "bundle-analyzer": "webpack-bundle-analyzer public/stats.json",
    "deploy": "yarn run deploy:sentry-upload && yarn run deploy:cleanup-assets",
    "upload": "gulp upload --cwd ./ --gulpfile node_modules/gousto-gulp/gulpfile.js/tasks/upload.js",
    "deploy:cleanup-assets": "cd public && rm -f *.js.map && cd ..",
    "eslint": "node_modules/.bin/eslint ."
  },
  "dependencies": {
    "actual": "0.4.0",
    "apollo-cache-inmemory": "1.3.8",
    "apollo-client": "2.4.2",
    "apollo-link": "1.0.3",
    "apollo-link-context": "1.0.3",
    "apollo-link-http": "1.2.0",
    "babel-polyfill": "6.26.0",
