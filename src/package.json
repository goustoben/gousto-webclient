  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/[^.]*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "libs/goustouicomponents"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(jpg|jpeg|png|gif|eot|otf|ico|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/.fileTransformer.js"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js}",
      "<rootDir>/server/**/*.{js}"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>",
      "<rootDir>/src",
      "<rootDir>/src/components"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/.setup.js",
    "globals": {
      "__DEV__": false,
      "__PROD__": true,
      "__HMR__": false,
      "__SERVER__": false,
      "__CLIENT__": true,
      "__CLOUDFRONT_URL__": "test.com",
      "__CLIENT_PROTOCOL__": "https",
      "__CHECKOUT_PK__": "",
      "__ENV__": "production",
      "__DOMAIN__": "gousto.local",
      "__TEST__": true,
      "process.env.NODE_ENV": "production",
      "__GIT_HASH__": "production"
    }
  }
}
