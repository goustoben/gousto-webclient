{
  "name": "goustowebclient",
  "version": "0.0.1",
  "description": "Isomorphic Gousto WebClient",
  "main": "dist/server.js",
  "author": "Gousto",
  "license": "ISC",
  "sideEffects": "false",
  "scripts": {
    "dev": "NODE_CONFIG_ENV=development NODE_APP_INSTANCE=local npm run build:server && NODE_CONFIG_ENV=development NODE_APP_INSTANCE=local node dist/server.js",
    "postinstall": "bower install --force --allow-root",
    "start": "node dist/server.js",
    "start:with-static": "withStatic=true npm run start",
    "start:dev:inspect": "nodemon --inspect=0.0.0.0 dist/server.js",
    "start:dev": "mkdir -p dist && touch ./dist/server.js && nodemon dist/server.js",
    "start:devlocal": "nodemon dist/server.js",
    "watch:client": "webpack --watch --config config/webpack.client.js",
    "watch:server": "webpack --watch --config config/webpack.server.js",
    "watch": "NODE_CONFIG_ENV=\"${NODE_CONFIG_ENV:=development}\"  NODE_APP_INSTANCE=\"${NODE_APP_INSTANCE:=local}\" run-p watch:client watch:server",
    "build:dev": "NODE_CONFIG_ENV=\"${NODE_CONFIG_ENV:=development}\"  NODE_APP_INSTANCE=\"${NODE_APP_INSTANCE:=local}\" run-p build:server build:client",
    "build:client": "webpack --config config/webpack.client.js --config-name=client --bail",
    "build:server": "webpack --config config/webpack.server.js --config-name=server --bail",
    "build:client:prod": "rm -rf public/* && NODE_ENV=production webpack --config config/webpack.client.js --bail",
    "build:server:prod": "rm -rf dist/* && NODE_ENV=production webpack --config config/webpack.server.js --bail",
    "build": "rm -rf dist/* && rm -rf public/* && NODE_CONFIG_ENV=\"${NODE_CONFIG_ENV:=production}\"  NODE_APP_INSTANCE=\"${NODE_APP_INSTANCE:=live}\" NODE_ENV=production run-p build:server build:client",
    "build:docker:dev": "rm -rf dist/* && rm -rf public/* && NODE_ENV=production npm_config_gousto_webclient_environment_name=local npm_config_gousto_webclient_domain=gousto.local run-p build:server build:client",
    "test:jest": "jest",
    "test:jest:ci": "jest --ci -w=4",
    "test:jest:one": "jest",
    "test:jest:one:watch": "jest --watch",
    "test:jest:watch": "jest --watchAll",
    "test:jest:coverage": "jest --coverage",
    "test:jest:ci:coverage": "jest --ci -w=2 --coverage",
    "bundle-analyzer": "webpack-bundle-analyzer public/stats.json",
    "deploy": "npm run deploy:cleanup-assets",
    "upload": "gulp upload --cwd ./ --gulpfile node_modules/gousto-gulp/gulpfile.js/tasks/upload.js",
    "deploy:cleanup-assets": "cd public && rm -f *.js.map && cd ..",
    "eslint": "eslint .",
    "codeowners:report": "node ./scripts/codeowners-report/codeowners-report.js",
    "codeowners:unowned": "node ./scripts/codeowners-report/codeowners-report.js --unowned"
  },
  "dependencies": {
    "@axe-core/react": "^4.1.1",
    "@optimizely/optimizely-sdk": "^4.1.0",
    "actual": "0.4.0",
    "airbnb-prop-types": "^2.16.0",
    "babel-polyfill": "6.26.0",
    "bower": "1.8.8",
    "classnames": "2.2.6",
    "cookies-js": "1.2.3",
    "dompurify": "^2.0.8",
    "gousto-lumberjack-js": "git@github.com:Gousto/lumberjack-js-package.git#v1.0.4",
    "immutable": "3.8.2",
    "isomorphic-fetch": "2.2.1",
    "jsonfile": "4.0.0",
    "koa": "2.5.1",
    "koa-body": "2.5.0",
    "koa-convert": "1.2.0",
    "koa-mount": "2.0.0",
    "koa-static": "3.0.0",
    "libphonenumber-js": "^1.9.23",
    "moment": "2.22.2",
    "performance-now": "^2.1.0",
    "perfume.js": "^6.2.0",
    "promise-timeout": "^1.3.0",
    "prop-types": "15.7.2",
    "qs": "6.5.2",
    "query-string": "4.2.3",
    "rc-tooltip": "4.0.3",
    "react": "17.0.2",
    "react-async-script": "0.9.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "17.0.2",
    "react-helmet": "5.0.3",
    "react-immutable-proptypes": "2.1.0",
    "react-lazyload": "2.3.0",
    "react-markdown": "2.5.0",
    "react-media": "^1.10.0",
    "react-modal": "^3.12.1",
    "react-nl2br": "0.2.0",
    "react-portal": "^4.2.1",
    "react-redux": "5.0.6",
    "react-router": "3.2.1",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "0.4.3",
    "react-select": "^1.2.1",
    "react-slick": "0.25.2",
    "react-transition-group": "^2.9.0",
    "react-use": "^15.3.4",
    "redux": "4.0.5",
    "redux-form": "^8.3.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "scroll": "2.0.3",
    "scroll-into-view": "1.8.0",
    "seedrandom": "2.4.3",
    "swr": "^0.5.6",
    "transit-immutable-js": "0.7.0",
    "transit-js": "0.8.861",
    "url": "0.11.0",
    "uuid": "3.3.2",
    "validator": "10.4.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "codeowners": "^4.2.0",
    "concat-stream": "^2.0.0",
    "css-loader": "1.0.1",
    "cssnano": "4.1.8",
    "cssnano-preset-advanced": "4.0.6",
    "enzyme": "3.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.4.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "gousto-gulp": "git@github.com:Gousto/gousto-gulp.git#3.4.1",
    "husky": "^4.2.2",
    "identity-obj-proxy": "3.0.0",
    "ignore": "^5.1.8",
    "image-webpack-loader": "4.6.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "11.3.0",
    "json5": "^2.2.0",
    "koa-webpack": "^6.0.0",
    "lodash-webpack-plugin": "0.11.5",
    "mockdate": "^2.0.5",
    "msw": "^0.33.0",
    "node-config": "npm:config@^3.3.3",
    "node-sass": "^4.13.1",
    "nodemon": "1.18.3",
    "normalize.css": "^8.0.1",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss": "7.0.7",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3",
    "postcss-nested": "4.1.1",
    "postcss-preset-env": "6.5.0",
    "postcss-reporter": "6",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "1.1.1",
    "request": "^2.88.0",
    "sass-loader": "7.1.0",
    "simple-progress-webpack-plugin": "1.1.2",
    "sinon": "2.1.0",
    "source-map-support": "0.5.9",
    "speed-measure-webpack-plugin": "^1.3.1",
    "spinkit": "1.2.5",
    "style-loader": "0.23.1",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.1.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-node-externals": "1.7.2"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ],
    "testMatch": [
      "**/__tests__/**/[^.]*.js?(x)",
      "**/?(*.)(test).js?(x)"
    ],
    "moduleNameMapper": {
      "fixtures/(.*)$": "<rootDir>/../tests/regression/fixtures/$1",
      "goustouicomponents": "<rootDir>/libs/goustouicomponents/src/main",
      "zest/(.*)$": "<rootDir>/libs/goustouicomponents/dist/$1",
      "gousto-config": "<rootDir>/libs/goustouicomponents/src/gousto-config",
      "design-language/icons/(.*)$": "<rootDir>/libs/goustouicomponents/src/design-language/icons/$1",
      "design-language/media/(.*)$": "<rootDir>/libs/goustouicomponents/src/design-language/media/$1",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "libs/goustouicomponents"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|ico|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/.fileTransformer.js"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js}",
      "<rootDir>/server/**/*.{js}",
      "!<rootDir>/src/scripts/**/*",
      "!<rootDir>/src/**/config/*.{js}",
      "!<rootDir>/src/_testing/**/*"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>",
      "<rootDir>/src",
      "<rootDir>/src/components"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/.setup.js",
    "globals": {
      "__DEV__": false,
      "__PROD__": true,
      "__HMR__": false,
      "__SERVER__": false,
      "__CLIENT__": true,
      "__CLOUDFRONT_URL__": "test.com",
      "__CLIENT_PROTOCOL__": "https",
      "__CHECKOUT_PK__": "",
      "__RECAPTCHA_RAF_PUBK__": "",
      "__RECAPTCHA_RAF_PVTK__": "",
      "__ENV__": "production",
      "__API_ENV__": "production",
      "__RUNNING_ENV__": "live",
      "__DOMAIN__": "gousto.local",
      "__TEST__": true,
      "process.env.NODE_ENV": "production",
      "__GIT_HASH__": "production",
      "__ENDPOINTS__": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm audit --audit-level=critical"
    }
  },
  "engines": {
    "node": "12.x"
  }
}
