    "graphql-anywhere": "4.1.22",
    "immutable": "3.8.2",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "11.3.0",
    "json-loader": "0.5.7",
    "jsonfile": "4.0.0",
    "koa": "2.5.1",
    "koa-body": "2.5.0",
    "koa-bodyparser": "3",
    "koa-convert": "1.2.0",
    "koa-mount": "2.0.0",
    "koa-static": "3.0.0",
    "moment": "2.22.2",
    "qs": "6.5.2",
    "query-string": "4.2.3",
    "prop-types": "15.6.2",
