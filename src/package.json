    "scroll": "2.0.3",
    "scroll-into-view": "1.8.0",
    "seedrandom": "2.4.3",
    "swr": "^0.5.6",
    "transit-immutable-js": "0.7.0",
    "transit-js": "0.8.861",
    "url": "0.11.0",
    "uuid": "3.3.2",
    "validator": "10.4.0"
  },
