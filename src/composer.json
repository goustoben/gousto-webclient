{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"anlutro/curl": "0.6.1",
		"fideloper/proxy": "dev-master",
		"jenssegers/agent": "dev-master",
		"guzzlehttp/guzzle": "~6.0",
		"gousto/fallback-session": "^1.0@dev",
		"phpunit/phpunit": "^4.8",
		"gousto/oauth2": "dev-master",
		"gousto/api-client": "dev-master"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:Gousto/FallbackSession.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Gousto/Oauth2.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:Gousto/ApiClient.git"
		}
	],
	"require-dev": {
		"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/packages",
			"app/filters",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
