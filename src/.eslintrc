{
  "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:you-dont-need-lodash-underscore/compatible",
      "plugin:jsx-a11y/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "react/no-children-prop": 2,
      "react/prop-types": 2,
      "newline-before-return": 2,
      "no-sparse-arrays": 2,
      "no-multi-spaces": 2,
      "no-multiple-empty-lines": [2, { "max": 1}],
      "indent": [2, 2, {"ignoredNodes": ["JSXElement"]}],
      "semi": [2, "never"],
      "no-shadow": [2, {
        "allow": ["resolve", "reject", "done", "cb", "e", "err", "error", "mapStateToProps"]
      }],
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 0,
      "import/newline-after-import": 0,
      "react/jsx-curly-spacing": 0,
      "react/jsx-indent-props": 0,
      "react/jsx-filename-extension": 0,
      "object-curly-spacing": 0,
      "array-bracket-spacing": 0,
      "computed-property-spacing": 0,
      "comma-spacing": 0,
      "space-in-parens": 0,
      "function-paren-newline": 0,
      "object-curly-newline": 0,
      "arrow-parens": 0,
      "max-len": 0,
      "implicit-arrow-linebreak": 0,
      "no-else-return": 0,
      "space-unary-ops": 0,
      "comma-dangle": 0,
      //beginning of gousto extended warnings
      "import/no-default-export" : 1,
      "no-underscore-dangle": [1, {
        "allow": ["__initialState__"]
      }],
      "no-tabs": 1,
      "no-mixed-spaces-and-tabs" : 1,
      "no-restricted-globals": 1,
      "eol-last": 1,
      "new-cap": 1,
      "new-parens":1,
      "prefer-destructuring": 1,
      "operator-linebreak": 1,
      "no-plusplus": [1, { "allowForLoopAfterthoughts": true }],
      "lines-between-class-members": 1,
      "comma-style": 1,
      "padded-blocks": 2,
      "prefer-const": 1,
      "class-methods-use-this": 1,
      //end of gousto extended warnings
      //beginning of warnings that are recommended as errors by our eslint plugins
      "import/no-dynamic-require" : 1,
      "import/no-self-import" : 1,
      "import/no-extraneous-dependencies": 1,
      "react/jsx-key": 1,
      "react/require-default-props": 1,
      "react/no-unused-prop-types": 2,
      "react/destructuring-assignment": 1,
      "react/forbid-prop-types": 1,
      "react/no-deprecated": 1,
      "react/jsx-one-expression-per-line": 1,
      "react/jsx-closing-tag-location": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/jsx-curly-brace-presence": 1,
      "react/default-props-match-prop-types": 1,
      "react/style-prop-object": 1,
      "react/display-name": 1,
      "react/no-unescaped-entities": 1,
      "react/no-array-index-key": 1,
      "react/no-typos": 1,
      "react/no-string-refs":1,
      "react/jsx-no-target-blank": [1, { "enforceDynamicLinks": "always" }],
      "react/jsx-props-no-multi-spaces": 1,
      "react/no-redundant-should-component-update": 1,
      "react/no-find-dom-node": 1,
      "react/no-unused-state": 1,
      "react/no-access-state-in-setstate": 1,
      "jsx-a11y/click-events-have-key-events": 1,
      "jsx-a11y/no-static-element-interactions": 1,
      "jsx-a11y/anchor-is-valid": 1,
      "jsx-a11y/label-has-associated-control": 1,
      "jsx-a11y/label-has-for": 1,
      "jsx-a11y/anchor-has-content": 1,
      "jsx-a11y/no-noninteractive-element-interactions": 1,
      "no-useless-escape": 1,
      "no-extra-boolean-cast": 1,
      "no-undef": 1,
      "no-control-regex": 1
  },
  "env" : {
    "jasmine": true
  },
  "globals": {
    "document": true,
    "navigator": true,
    "require": false,
    "window": true,
    "__DEV__": false,
    "__PROD__": false,
    "__HMR__": false,
    "__SERVER__": false,
    "__CLIENT__": false,
    "__ENV__": false,
    "__DOMAIN__": false,
    "__CLIENT_PROTOCOL__": false,
    "__CLOUDFRONT_URL__": false,
    "__CHECKOUT_PK__": false,
    "__TEST__": false,
    "__GIT_HASH__": false,
    "ga": false
  },
  "overrides": [
    {
      "files": [
        "**/*.test.js"
      ],
      "env": {
        "jest": true
      },
      "rules": {
        "react/display-name": 0
      }
    }
  ]
}
