{
  "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-multiple-empty-lines": ["error", { "max": 1}],
      "object-curly-spacing": 0,
      "array-bracket-spacing": 0,
      "computed-property-spacing": 0,
      "comma-spacing": 0,
      "space-in-parens": 0,
      "semi": ["error", "never"],
      "function-paren-newline": 0,
      "import/no-unresolved": 0,
      "import/named" : 1,
      "import/no-amd" : 1,
      "import/no-nodejs-modules" : 1,
      "import/no-commonjs" : 1,
      "import/namespace" : 2,
      "import/no-dynamic-require" : 1,
      "import/no-webpack-loader-syntax" : 2,
      "import/no-self-import" : 1,
      "import/no-cycle" : 2,
      "import/no-useless-path-segments" : 2,
      "import/order": 1,
      "import/export" : 1,
      "import/first" : 1,
      "import/exports-last" : 1,
      "import/no-duplicates" : 2,
      "import/no-named-default" : 2,
      "import/no-default-export" : 1,
      "import/no-anonymous-default-export" : 1,
      "react/jsx-indent-props": ["warning", 2],
      "react/jsx-key": 1,
      "react/prefer-stateless-function": 1,
      "import/no-mutable-exports": 1,
      "react/require-default-props": 0,
      "indent": ["error", 2, {"ignoredNodes": ["JSXElement"]}],
      "max-len": "off",
      "no-shadow": ["error", {
        "allow": ["resolve", "reject", "done", "cb", "e", "err", "error", "mapStateToProps"]
      }],
      "no-underscore-dangle": [1, {
        "allow": ["__initialState__"]
      }],
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-indent": ["error", 2],
      "no-tabs": 1,
      "no-mixed-spaces-and-tabs" : 1,
      "react/prop-types": 1,
      "react/no-unused-prop-types": 1,
      "react/destructuring-assignment": 1,
      "react/forbid-prop-types": 1,
      "jsx-a11y/no-static-element-interactions": 0,
      "new-cap": 1,
      "newline-before-return": "error",
      "arrow-parens": 0,
      "new-parens":1,
      "implicit-arrow-linebreak": 0,
      "no-else-return": 0,
      "prefer-destructuring": 1,
      "no-useless-escape": 1,
      "no-plusplus": 0,
      "operator-linebreak": 0,
      "object-curly-newline": 0,
      "react/no-deprecated": 1,
      "react/jsx-wrap-multilines": 0,
      "react/jsx-one-expression-per-line": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "react/jsx-closing-tag-location": 1,
      "comma-dangle": 0,
      "react/jsx-tag-spacing": 0,
      "react/self-closing-comp": 0,
      "react/self-closing-import/no-dynamic-require": 1,
      "lines-between-class-members": 0,
      "jsx-a11y/label-has-for": 0,
      "react/sort-comp": 0,
      "react/jsx-curly-brace-presence": 0,
      "react/default-props-match-prop-types": 0,
      "comma-style": 0,
      "react/style-prop-object": 0,
      "react/display-name": 1,
      "padded-blocks": 0,
      "react/no-unescaped-entities": 1,
      "react/no-array-index-key": 1,
      "no-multi-spaces": 2,
      "react/no-typos": 1,
      "react/no-string-refs":1,
      "react/jsx-no-target-blank": [1, { "enforceDynamicLinks": "always" }],
      "no-prototype-builtins": 1,
      "no-extra-boolean-cast": 0,
      "import/extensions": 0,
      "prefer-const": 1,
      "class-methods-use-this": 1,
      "space-unary-ops": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "react/jsx-props-no-multi-spaces": 0,
      "react/no-redundant-should-component-update": 1,
      "import/no-named-as-default-member": 1,
      "import/newline-after-import": 0,
      "no-lonely-if": 0,
      "react/no-find-dom-node": 1,
      "react/no-unused-state": 1,
      "no-unused-vars": 1,
      "import/no-extraneous-dependencies": 1,
      "no-return-await": 1,
      "react/jsx-no-duplicate-props": 1,
      "no-undef": 1,
      "prefer-promise-reject-errors": 1,
      "react/button-has-type": 1,
      "no-restricted-globals": 1,
      "no-mixed-operators": 1,
      "react/jsx-curly-spacing": 0,
      "jsx-a11y/interactive-supports-focus": 1,
      "react/no-access-state-in-setstate": 1,
      "jsx-a11y/no-autofocus": 1,
      "jsx-a11y/anchor-has-content": 1,
      "jsx-a11y/no-noninteractive-element-interactions": 1,
      "no-bitwise": 1,
      "no-control-regex": 1,
      "react/no-children-prop": 2,
      "jsx-a11y/label-has-associated-control": 0
    
  },
  "env" : {
    "jasmine": true
  },
  "globals": {
    "document": true,
    "navigator": true,
    "require": false,
    "window": true,
    "__DEV__": false,
    "__PROD__": false,
    "__HMR__": false,
    "__SERVER__": false,
    "__CLIENT__": false,
    "__ENV__": false,
    "__DOMAIN__": false,
    "__CLIENT_PROTOCOL__": false,
    "__CLOUDFRONT_URL__": false,
    "__TEST__": false,
    "__GIT_HASH__": false
  }
}
