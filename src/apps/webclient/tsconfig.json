{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "checkJs": false,
    "jsx": "react",
    "lib": ["ES2015", "ES2015.Promise", "dom"],
    "outDir": "../lib/client",
    "types": ["jest", "node", "@emotion/react/types/css-prop"],
    "paths": {
      "_testing/*": ["./src/_testing/*"],
      "*": ["./src/components/*", "../libs/goustouicomponents/./*"],
      "actions": ["./src/actions/index.js"],
      "actions/*": ["./src/actions/*"],
      "apis/*": ["./src/apis/*"],
      "client/*": ["./src/client/*"],
      "components/*": ["./src/components/*"],
      "config/*": ["./src/config/*"],
      "containers/*": ["./src/containers/*"],
      "design-language": ["../libs/goustouicomponents/dist/design-language"],
      "fixtures": ["../../../tests/regression/fixtures"],
      "goustouicomponents": ["../libs/goustouicomponents/./main"],
      "hoc/*": ["./src/hoc/*"],
      "hooks/*": ["./src/hooks/*"],
      "jsdom": ["fallbacks/jsdom"],
      "layouts/*": ["./src/layouts/*"],
      "media/*": ["./src/media/*"],
      "middlewares/*": ["./src/middlewares/*"],
      "reducers/*": ["./src/reducers/*"],
      "routes": ["./src/routes/index.js"],
      "routes/*": ["./src/routes/*"],
      "selectors/*": ["./src/selectors/*"],
      "server": ["./server/*"],
      "styles/*": ["./src/styles/*", "../libs/goustouicomponents/dist/styles/*"],
      "utils/*": ["./src/utils/*"],
      "validations/*": ["./src/validations/*"],
      "zest": ["../libs/goustouicomponents/dist"]
    },
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "./src/**/*"
  ]
}
