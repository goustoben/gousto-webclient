{
  "name": "webclient",
  "version": "0.0.1",
  "description": "Main webclient app",
  "main": "dist/server.js",
  "author": "Gousto",
  "license": "UNLICENSED",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "NODE_ENV=development yarn build:server:dev && yarn start",
    "dev:server": "NODE_ENV=development DOTENV_CONFIG_PATH=./server/.env nodemon -r dotenv/config dist/server.js",
    "bower-install": "bower install --force --allow-root",
    "start": "NODE_ENV=development DOTENV_CONFIG_PATH=./server/.env node -r dotenv/config dist/server.js",
    "start:with-static": "withStatic=true yarn start",
    "start:dev:inspect": "nodemon --inspect=0.0.0.0 dist/server.js",
    "start:devlocal": "nodemon dist/server.js",
    "watch:server": "NODE_ENV=development webpack --watch --config config/webpack.server.js",
    "watch:client": "NODE_ENV=development webpack --watch --config config/webpack.client.js",
    "watch": "run-p watch:client watch:server",
    "build:dev": "run-p build:server build:client",
    "build:client": "webpack --config config/webpack.client.js --config-name=client --bail",
    "build:server": "webpack --config config/webpack.server.js --config-name=server --bail",
    "build:server:dev": "webpack --config config/webpack.server.development.js --config-name=server --bail",
    "build": "rm -rf dist/* && rm -rf public/* && NODE_ENV=production run-p -l build:server build:client",
    "generate-libphonenumber-metadata": "libphonenumber-metadata-generator src/utils/phoneNumber/phone-metadata.json --countries GB --extended",
    "test:jest": "jest",
    "test:jest:ci": "jest --ci --detectOpenHandles --forceExit --logHeapUsage --silent",
    "test:jest:one:watch": "jest --watch",
    "test:jest:watch": "jest --watchAll",
    "test:jest:coverage": "jest --coverage",
    "test:jest:ci:coverage": "jest --ci --detectOpenHandles --forceExit --logHeapUsage --silent --coverage",
    "bundle-analyzer": "webpack-bundle-analyzer public/stats.json",
    "eslint": "eslint .",
    "eslint:ci": "eslint --format=json",
    "codeowners:report": "node ./scripts/codeowners-report/codeowners-report.js",
    "codeowners:unowned": "node ./scripts/codeowners-report/codeowners-report.js --unowned",
    "clean-install": "rm -rf ./node_modules && yarn install",
    "typecheck:tests": "tsc -p ./tsconfig.tests.json",
    "datadog:upload-sourcemaps": "datadog-ci sourcemaps upload $BUILD_DIR --service gousto-webclient --release-version $CIRCLE_BUILD_NUM --minified-path-prefix $ASSET_URL_PREFIX",
    "datadog:upload-report:e2e": "datadog-ci junit upload --service gousto-webclient ../../../tests/e2e/reports",
    "prettier": "prettier -w ./"
  },
